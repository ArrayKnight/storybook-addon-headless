{
    "name": "storybook-addon-headless",
    "author": "Ray Knight <array.knight+headless@gmail.com>",
    "description": "Storybook addon to preview content from a headless CMS in components",
    "version": "0.0.0-development",
    "license": "MIT",
    "scripts": {
        "preinstall": "rm -rf node_modules && rm -f package-lock.json && rm -f yarn.lock",
        "prebuild": "npm run clean",
        "build": "npm-run-all build:*",
        "build:dist": "npm run rollup",
        "build:docs": "npm run storybook:build",
        "clean": "rm -rf dist && rm -rf docs",
        "commit": "npm run lint && git add -i && npx git-cz",
        "lint": "eslint \"!(dist|node_modules)/**/*.{js,jsx,ts,tsx}\"",
        "rollup": "rollup -c",
        "rollup:watch": "rollup -c -w",
        "semantic-release": "semantic-release",
        "prestart": "npm run clean",
        "start": "npm-run-all -p -r rollup:watch storybook:start:dist",
        "start:dev": "npm run storybook:start:dev",
        "storybook:start:dev": "start-storybook -c .storybook-dev",
        "storybook:start:dist": "wait-on dist/register.js && start-storybook -c .storybook-dist",
        "storybook:build": "wait-on dist/register.js && build-storybook -c .storybook-dist -o docs",
        "upgrade": "npm-run-all upgrade:*",
        "upgrade:dep": "npm-check --update --production",
        "upgrade:dev": "npm-check --update --dev-only --save-exact"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "post-commit": "git push -u origin $(git rev-parse --abbrev-ref HEAD)"
        }
    },
    "lint-staged": {
        "**/*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@material-ui/core": "4.9.3",
        "@storybook/addon-actions": "6.0.0-alpha.15",
        "@storybook/addon-docs": "6.0.0-alpha.15",
        "@storybook/addon-knobs": "6.0.0-alpha.15",
        "@storybook/react": "6.0.0-alpha.15",
        "@types/ajv-keywords": "3.4.0",
        "@types/react": "16.9.21",
        "@typescript-eslint/eslint-plugin": "2.20.0",
        "@typescript-eslint/parser": "2.20.0",
        "babel-loader": "8.0.6",
        "babel-preset-react-app": "9.1.1",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-config-react": "1.1.7",
        "eslint-plugin-prettier": "3.1.2",
        "eslint-plugin-react": "7.18.3",
        "eslint-plugin-react-hooks": "2.4.0",
        "husky": "4.2.3",
        "lint-staged": "10.0.7",
        "npm-check": "5.9.0",
        "npm-run-all": "4.1.5",
        "prettier": "1.19.1",
        "react-is": "16.12.0",
        "rollup": "1.31.1",
        "rollup-plugin-typescript2": "0.26.0",
        "semantic-release": "17.0.4",
        "typescript": "3.8.2",
        "wait-on": "4.0.0"
    },
    "dependencies": {
        "@apollo/client": "^3.0.0-beta.36",
        "@storybook/addons": "^6.0.0-alpha.15",
        "@storybook/api": "^6.0.0-alpha.15",
        "@storybook/components": "^6.0.0-alpha.15",
        "@storybook/core": "^6.0.0-alpha.15",
        "@storybook/core-events": "^6.0.0-alpha.15",
        "@storybook/theming": "^6.0.0-alpha.15",
        "ajv": "^6.11.0",
        "ajv-keywords": "^3.4.1",
        "axios": "^0.19.1",
        "change-case": "^4.1.1",
        "date-fns": "^2.9.0",
        "downshift": "^5.0.3",
        "graphql": "^14.6.0",
        "react": "^16.12.0",
        "react-json-view": "^1.19.1"
    },
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/ArrayKnight/storybook-addon-headless.git"
    },
    "homepage": "https://storybook-addon-headless.netlify.com/",
    "bugs": {
        "url": "https://github.com/ArrayKnight/storybook-addon-headless/issues"
    },
    "release": {
        "branch": "master"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "storybook",
        "storybookjs",
        "storybook-addon",
        "addon",
        "headless",
        "headless-cms",
        "rest",
        "graphql"
    ]
}
